public class Main {
    public static void main(String[] args) {
        // Specify the path of the Excel file to be read
        String excelFilePath = "example.xlsx";  // Provide the actual path

        // Create an instance of ExcelFileReader
        ExcelFileReader excelFileReader = new ExcelFileReader(excelFilePath);

        // Read the Excel file
        excelFileReader.readExcelFile();
    }
}
